<div class="row" style="margin : 1%">
    <h3>Chỉnh sửa đơn hàng</h3>
    <div class="col-sm-4">
        {{!-- Form chỉnh sửa đơn --}}
        <div class="form-group">

            <label for="usr">Đơn hàng: </label>
            <input id="ordername" type="text" class="form-control" id="usr">
        </div>

        <div class="form-group">
            <label for="pwd">Đi từ:</label>
            <input id="from" onchange="getLocationFrom()" type="text" class="form-control" id="pwd">
        </div>

        <div class="form-group">
            <label for="pwd">Đi đến:</label>
            <input id="to" type="text" onchange="getLocationTo()" class="form-control" id="pwd">
        </div>

        <div class="form-group">
            <label for="pwd">Giá hàng (VNĐ):</label>
            <input id="price" type="text" class="form-control" id="pwd">
        </div>

        <div class="form-group">
            <label for="pwd">Giá ship (VNĐ):</label>
            <input id="priceShip" type="text" class="form-control" id="pwd">
        </div>

        {{!-- Nút sửa đơn --}}
        <div><button id="btnEditOrder" type="button" class="btn btn-success center-block">Sửa đơn</button></div>

    </div>

        <div class="col-sm-8">
            <div class="row" style="width:100%; margin:0;">
                <div class="col-sm-1" style="height: 400px;"></div>

                <div class="col-sm-10" id="mapEditOrder" style="height:400px"></div>

                <div class="col-sm-1" style="height: 400px;"></div>
            </div>
        </div>
</div>

<script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDyVm6Ku29H8jLCuwK1xDypxLu3FgQb4Qs">
</script>


<script src="js/editOrder.js"></script>

